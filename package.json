{
	"name": "school-idol-collection",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"prepare": "playwright install && husky install && patch-package",
		"dev": "vite --host 0.0.0.0 dev",
		"sync": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:import": "TEST=true vite-node src/scripts/dbImportExport.ts import",
		"db:export": "TEST=true vite-node src/scripts/dbImportExport.ts export",
		"test": "playwright test",
		"prebuild": "TEST=true vite-node src/scripts/collectCardImages.ts",
		"build": "vite build",
		"preview": "vite --host 0.0.0.0 preview",
		"start": "node -r dotenv/config build"
	},
	"devDependencies": {
		"@playwright/test": "^1.38.1",
		"@sequelize/core": "^7.0.0-alpha.23",
		"@splidejs/svelte-splide": "^0.2.9",
		"@sveltejs/adapter-node": "^1.1.0",
		"@sveltejs/kit": "^1.20.4",
		"@trivago/prettier-plugin-sort-imports": "^4.2.1",
		"@types/jsdom": "^21.1.1",
		"@types/node": "^20.8.2",
		"@types/probe-image-size": "^7.2.0",
		"@types/uuid": "^9.0.4",
		"autoprefixer": "^10.4.13",
		"file-type": "^18.7.0",
		"husky": "^8.0.3",
		"jsdom": "^21.1.2",
		"mariadb": "^3.2.2",
		"patch-package": "^8.0.0",
		"postcss": "^8.4.20",
		"postcss-nesting": "^12.0.1",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.5.6",
		"probe-image-size": "^7.2.3",
		"sqlite3": "^5.1.6",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"svelte-reduced-motion": "^1.0.5",
		"svooltip": "^0.7.2",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"tw-colors": "^3.3.0",
		"typescript": "^5.0.0",
		"uuid": "^9.0.1",
		"vite": "^4.0.0",
		"vite-node": "^0.34.6"
	},
	"dependencies": {
		"dotenv": "^16.3.1"
	}
}
