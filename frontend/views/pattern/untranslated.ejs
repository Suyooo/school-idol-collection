<%- include("../partials/header", {"title": "Untranslated", "styles": []}); %>
<div class="content">
    <h5><%= untranslated.length %> Untranslated Skill(s)</h5>
    <div class="panel">
        <div class="panel-inner">
            <table>
                <thead>
                <tr>
                    <th>Card</th>
                    <th>JPN</th>
                    <th>Pattern</th>
                </tr>
                </thead>
                <tbody>
                <% untranslated.forEach(function(a) { %>
                    <tr>
                        <% if (a.hasOwnProperty("cardNo")) { %>
                            <th><a href="/card/<%= a.cardNo %>"><%= a.cardNo %></a></th>
                        <% } else { %>
                            <th><a href="/card/<%= a.firstCardNo %>">Group #<%= a.groupId %></a></th>
                        <% } %>
                        <td><%- a.skill.replace(/\n/g, "<br>") %></td>
                        <% if (a.hasOwnProperty("cardNo")) { %>
                            <td><a href="/pattern/create/<%= a.cardNo %>/<%= a.line %>">Create</a></td>
                        <% } else { %>
                            <td><a href="/pattern/create">Create (Blank)</a></td>
                        <% } %>
                    </tr>
                <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</div>
<%- include("../partials/footer", {"jquery": false, "scripts": []}); %>